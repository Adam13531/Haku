{
  "name": "haku",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:migrate": "dotenv -c -- bash -c 'DB_URL=$MIGRATE_DB_URL pnpm prisma migrate dev'",
    "dev:reset": "dotenv -c -- bash -c 'DB_URL=$MIGRATE_DB_URL pnpm prisma migrate reset'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint -d . --max-warnings=0",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "0.5.2-next.19",
    "@prisma/client": "3.6.0",
    "ky": "0.28.7",
    "next": "12.0.7",
    "next-auth": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.21.0",
    "react-query": "3.34.2",
    "status-code-enum": "1.0.0",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "autoprefixer": "10.4.0",
    "dotenv-cli": "4.1.1",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.5",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "postcss": "8.4.4",
    "prettier": "2.5.0",
    "prisma": "3.6.0",
    "tailwindcss": "3.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
