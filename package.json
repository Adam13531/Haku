{
  "name": "haku",
  "description": "Craft, consolidate and tackle your notebooks and to-do lists all in one place.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm build:sw && next dev",
    "dev:migrate": "dotenv -c -- ts-node prisma/migrate.ts",
    "build": "next build && pnpm build:sw -- --prod",
    "build:sw": "ts-node sw-build.ts",
    "build:analyze": "ANALYZE=true pnpm build",
    "start": "next start",
    "lint": "next lint -d . --max-warnings=0 && tsc --noEmit && tsc --noEmit -p src/sw/tsconfig.json",
    "test:api": "TEST_TYPE=api jest -i",
    "test:client": "TEST_TYPE=client jest -i",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
